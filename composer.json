{
    "name": "rygilles/laravel-api-boilerplate",
    "description": "Laravel API Boilerplate project.",
    "keywords": ["framework", "laravel"," API", "boilerplate"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "algolia/algoliasearch-client-php": "^1.18",
        "alsofronie/eloquent-uuid": "^1.0",
        "barryvdh/laravel-cors": "^0.8.6",
        "barryvdh/laravel-debugbar": "^2.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "dingo/api": "1.0.*@dev",
        "doctrine/dbal": "^2.5",
        "felixkiss/uniquewith-validator": "^3.0",
        "filp/whoops": "^2.1",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^2.6",
        "rygilles/laravel-apidoc-generator": "@dev",
        "rygilles/laravel-openapi-schema-generator": "@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
